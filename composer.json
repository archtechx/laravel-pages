{
    "name": "archtechx/laravel-pages",
    "description": "Easily add routes to your Laravel app by creating Markdown or Blade files.",
    "keywords": ["laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Å tancl",
            "email": "samuel@archte.ch"
        }
    ],
    "autoload": {
        "psr-4": {
            "ArchTech\\Pages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArchTech\\Pages\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.24|^9.0|^10.0",
        "archtechx/laravel-seo": "^0.6",
        "ryangjchandler/orbit": "^1.2",
        "illuminate/routing": "^8.53|^9.0|^10.0",
        "illuminate/database": "^8.53|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.9|^7.0|^8.0",
        "pestphp/pest": "^1.2|^2.0",
        "pestphp/pest-plugin-laravel": "^1.0|^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArchTech\\Pages\\PagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
