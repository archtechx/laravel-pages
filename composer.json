{
    "name": "archtechx/laravel-pages",
    "description": "Easily add routes to your Laravel app by creating Markdown or Blade files.",
    "keywords": [
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Å tancl",
            "email": "samuel@archte.ch"
        }
    ],
    "autoload": {
        "psr-4": {
            "ArchTech\\Pages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArchTech\\Pages\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "archtechx/laravel-seo": "^0.6",
        "ryangjchandler/orbit": "*",
        "illuminate/routing": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.2|^2.0",
        "pestphp/pest-plugin-laravel": "^1.0|^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArchTech\\Pages\\PagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
